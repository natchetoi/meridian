{#
/**
 * @file
 * Base+'s theme implementation to display a Product node in teaser tile view mode.
 */
#}
{%
  set classes = [
    'node',
    'node--type-' ~ node.bundle|clean_class,
    node.isPromoted() ? 'node--promoted',
    node.isSticky() ? 'node--sticky',
    not node.isPublished() ? 'node--unpublished',
    view_mode ? 'node--view-mode-' ~ view_mode|clean_class,
  ]
%}
{{ attach_library('baseplus/node-view-mode-teaser-tile') }}
{% set image = content.field_mt_prd_teaser_image %}
{% set teaser_tile_pre_content_first = content.field_mt_prd_tags %}
{% set teaser_tile_pre_content_second = null %}
{% set rendered_content = content|without('field_mt_prd_teaser_image', 'field_mt_prd_tags', 'field_mt_prd_price') %}
{% set teaser_tile_content = null %}
{% set teaser_tile_subtitle = content.field_mt_prd_price %}
{% set image_caption = null %}
{% set date_overlay = false %}
{% set submitted_day = null %}
{% set submitted_month = null %}
{% set submitted_year = null %}
{% set read_more = null %}
{% include '@baseplus/partials/_mt-teaser-tile-card.html.twig' %}
