{#
/**
 * @file
 * Base+'s theme implementation to display a Album node in teaser view mode.
 */
#}
{%
  set classes = [
    'node',
    'node--type-' ~ node.bundle|clean_class,
    node.isPromoted() ? 'node--promoted',
    node.isSticky() ? 'node--sticky',
    not node.isPublished() ? 'node--unpublished',
    view_mode ? 'node--view-mode-' ~ view_mode|clean_class,
    display_submitted ? 'node--display-submitted',
    'clearfix',
  ]
%}
{{ attach_library('baseplus/node-view-mode-teaser') }}
{{ attach_library('baseplus/node-field') }}
{% set image = null %}
{% set teaser_pre_content_first = null %}
{% set teaser_pre_content_second = null %}
{% set rendered_content = content %}
{% set submitted_day = null %}
{% set submitted_month = null %}
{% set submitted_year = null %}
{% include '@baseplus/partials/_mt-teaser.html.twig' %}
